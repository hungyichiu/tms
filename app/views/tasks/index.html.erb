<h1>Tasks List</h1>

<% @tasks.each do |task| %>
  <ul>
    <li><%= link_to task.title, task_path(task) %> <%= task.created_at %> <%= link_to "Edit", edit_task_path(task) %> <%= link_to "Delete", task_path(task), method:'delete',data: {confirm: "Are you sure? "} %></li>
  </ul>

<% end %>

<%= link_to "Add Task", new_task_path %>